import { Column, Entity, PrimaryGeneratedColumn } from "typeScript";
@Entity({name: 'booking'})

export class booking {
    @PrimaryGeneratedColumn()
    id: number; //this id will be auto-generated, don't enter any id in the swagger/postman

    @Column()
    name: string;

    @Column()
    Location: string;

    @Column()
    Date: string;

    @Column()
    Email: string;

    @Column()
    Guests: string;
  }